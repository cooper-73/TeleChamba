<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.telechamba">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Dark">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"> </activity>
        <activity android:name=".ProjectUserView" ></activity>
        <activity android:name=".ProjectOwnerView" ></activity>
        <activity android:name=".Contratar"></activity>
        <activity android:name=".Profile" ></activity>
        <activity android:name=".ConfirmEmail"></activity>
        <activity android:name=".CreateProjectActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Despedir"></activity>
    </application>
</manifest>
